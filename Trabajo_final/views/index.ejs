<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <script src="https://unpkg.com/boxicons@2.0.9/dist/boxicons.js"></script>
    <title>Registro</title>
</head>
<body>
    <div class="container">

        <table>
            <thead>
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">Tipo</th>
                    <th scope="col">Descripcion</th>
                    <th scope="col">ID</th>
                    <th scope="col">ID</th>
                    <th scope="col">ID</th>
                    <th scope="col">ID</th>
                    <th scope="col">ID</th>
                    <th scope="col">ID</th>
                    <th scope="col">ID</th>
                    <th scope="col">ID</th>
                    <th scope="col">nombre doc</th>
                    <th scope="col">Acciones</th>
                </tr>
            </thead>
            <tbody>
                
                <% results.forEach((tipo)=> { %>
                <tr>
                    <th><%= tipo.id %></th>
                    <th><%= tipo.nombres %></th>
                    <th><%= tipo.apellidos %></th>
                    <th><%= tipo.idtipodocumento %></th>
                    <th><%= tipo.documento %></th>
                    <th><%= tipo.ciudad %></th>
                    <th><%= tipo.fechanacimiento %></th>
                    <th><%= tipo.email %></th>
                    <th><%= tipo.telefono %></th>
                    <th><%= tipo.usuario %></th>
                    <th><%= tipo.contrasena %></th>
                    <th><%= tipo.nombre%></th>
                    <th>
                        <a href="/edit">EDIT</a>
                    </th>
                </tr>
                <% }) %>
            </tbody>
        </table>
    </div>
</body>
</html>